{
  "name": "spa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run timestamp && ng serve",
    "build": "ng build",
    "build-docker": "npm run timestamp && node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod -c=docker",
    "build-prod": "npm run timestamp && node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod -c=production && npm run fix-ngsw",
    "build-staging": "npm run timestamp && node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod -c=staging && npm run fix-ngsw",
    "build-test": "npm run timestamp && node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod -c=test --base-href /ssru/ && npm run fix-ngsw",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "timestamp": "node ./timestamp.js",
    "fix-ngsw": "node ./fix-ngsw.js"
  },
  "private": true,
  "dependencies": {
